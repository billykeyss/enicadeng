extends layout

block content
    section#section-projects.appear.clearfix
      .container.page-container
        .row
          .col-md-4.page-header-padding
            .page-header
              a(href='/') E N I C A
            nav.cl-effect-1
              .menu-item.alpha
                h4
                  a(href='/') Home
              .menu-item
                h4
                  a(href='projects.html') Projects
                ul
                  li
                    a(href='projects1.html') Project 1
                  li
                    a(href='projects2.html') Project 2
                  li
                    a(href='projects3.html') Project 3
              .menu-item
                h4
                  a(href='concept.html') Concept Art
                ul
                  li
                    a(href='#') Concept Art 1
                  li
                    a(href='#') Concept Art 2
                  li
                    a(href='#') Concept Art 3
              .menu-item
                h4
                  a(href='about.html') About
              .menu-item
                h4
                  a(href='contact.html') Contact
          .col-md-6
            .section-header
              h2.section-heading.animated(data-animation='bounceInUp') P R O J E C T &nbsp 1
    // Large modal
    .modal.fade.bs-example-modal-lg(tabindex='-1', role='dialog', aria-labelledby='myLargeModalLabel')
      .modal-dialog.modal-lg(style='width: 1200px')
        .modal-content
          .modal-content
            .modal-header
              button.close(type='button', data-dismiss='modal', aria-label='Close')
                span(aria-hidden='true') Ã—
              h4#myModalLabel.modal-title Modal title
            .modal-body
              #carousel-example-generic.carousel.slide(data-ride='carousel')
                // Indicators
                //
                  <ol class="carousel-indicators">
                  <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                  <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                  <li data-target="#carousel-example-generic" data-slide-to="2"></li>
                  </ol>
                // Wrapper for slides
                .carousel-inner(role='listbox')
                  .item.active
                    img(src='img/portfolio/img2-min.jpg', alt='...')
                    .carousel-caption
                      | OMG IMAGE 1
                  .item
                    img(src='img/portfolio/img1-min.jpg', alt='...')
                    .carousel-caption
                      | OMG ITS ANOTHER IMAGE!!!
                // Controls
                a.left.carousel-control(href='#carousel-example-generic', role='button', data-slide='prev')
                  span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
                  span.sr-only Previous
                a.right.carousel-control(href='#carousel-example-generic', role='button', data-slide='next')
                  span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
                  span.sr-only Next
            .modal-footer
              button.btn.btn-default(type='button', data-dismiss='modal') Close
              button.btn.btn-primary(type='button') Save changes
    script(src='js/modernizr-2.6.2-respond-1.1.0.min.js')
    script(src='js/jquery.js')
    script(src='js/jquery.easing.1.3.js')
    script(src='js/bootstrap.min.js')
    script(src='js/jquery.isotope.min.js')
    script(src='js/fancybox/jquery.fancybox.pack.js')
    script(src='js/skrollr.min.js')
    script(src='js/jquery.scrollTo-1.4.3.1-min.js')
    script(src='js/jquery.localscroll-1.2.7-min.js')
    script(src='js/stellar.js')
    script(src='js/jquery.appear.js')
    script(src='js/validate.js')
    script(src='js/main.js')
